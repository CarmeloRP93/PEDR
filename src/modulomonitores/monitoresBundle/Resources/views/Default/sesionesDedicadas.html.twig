{# empty Twig template #}
{% extends 'modulomonitoresmonitoresBundle::main.html.twig' %}

{% block title %} Vista de sesiones dedicadas {% endblock %}            
{% block contenido %}

<h1>Listado de sesiones de dedicadas</h1>
{% for sesion in sesiones %}
    <a href="{{url('modulomonitores_monitores_sesionDedicada', {'matricula': sesion.matricula})}}" class="fila {{cycle(["par","impar"], loop.index)}}"> {{ sesion.matricula }} - {{ sesion.nombre }} - {{ sesion.cliente }} - {{ sesion.estado}} </a>
{% endfor %}
<div class="accionesesion text-center" style="margin-top:5px; margin-left: 5px">
        <button style="height: 30px; width: 200px;" class="btn btn-success col-sm-1">Crear sesion dedicada</button>
    </div>
{% endblock %}