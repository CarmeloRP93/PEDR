{# empty Twig template #}
{% extends 'CriveroPruebaBundle::main.html.twig' %}

{% block title %} Vista de Clientes {% endblock %}
{% block contenido %}
<h1>Listado de Clientes</h1>
{% for alumno in alumnos %}
    <a href="{{url('crivero_prueba_alumno', {'matricula': alumno.matricula})}}" class="fila {{cycle(["par","impar"], loop.index)}}"> {{ alumno.matricula }} - {{ alumno.nombre }} </a>
{% endfor %}

{% endblock %}